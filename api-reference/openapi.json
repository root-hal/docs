        },
        "/v1/pdf/summarize": {
            "post": {
                "tags": [
                    "PDF"
                ],
                "summary": "Summarize PDF",
                "operationId": "summarize_v1_pdf_summarize_post",
                "requestBody": {
                    "content": {
                        "application/json": {
