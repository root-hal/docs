    "components": {
        "schemas": {
            "CorrectRequest": {
                "properties": {
                    "text": {
                        "type": "string",
                        "title": "Text",
                        "description": "The text content to be corrected",
                        "example": "i cant beleive its raning again today, my picknick plans are ruined agian!"
                    }
                },
                "type": "object",
                "required": [
                    "text"
                ],
                "title": "CorrectRequest"
            },
            "CorrectResponse": {
                "properties": {
                    "correction": {
                        "type": "string",
                        "title": "Correction",
                        "description": "The corrected text with grammar and spelling fixes",
                        "example": "..."
                    }
                },
                "type": "object",
                "required": [
                    "correction"
                ],
                "title": "CorrectResponse"
            },
            "ErrorResponse": {
                "properties": {
                    "type": {
                        "type": "string",
                        "title": "Type"
                    },
                    "status": {
                        "type": "integer",
                        "title": "Status"
                    },
                    "title": {
                        "type": "string",
                        "title": "Title"
                    },
                    "detail": {
                        "type": "string",
                        "title": "Detail"
                    },
                    "instance": {
                        "type": "string",
                        "title": "Instance"
                    }
                },
                "type": "object",
                "required": [
                    "type",
                    "status",
                    "title",
                    "detail",
                    "instance"
                ],
                "title": "ErrorResponse",
                "description": "RFC 7807 Problem Details error response schema",
                "example": {
                    "detail": "Access forbidden or source unavailable.",
                    "instance": "8428a19f-401e-47bc-b3f6-85f2bc3bed17",
                    "status": 403,
                    "title": "Source Not Available",
                    "type": "https://loor.ai/v1/url/summarize"
                }
            },
            "ParaphraseRequest": {
                "properties": {
                    "text": {
                        "type": "string",
                        "title": "Text",
                        "description": "The text content to be paraphrased",
                        "example": "My grandmother makes the most delicious cookies in the entire universe and everyone agrees!"
                    },
                    "tone": {
                        "type": "string",
                        "title": "Tone",
                        "description": "The desired tone for the paraphrased text (e.g., formal, casual, professional)",
                        "example": "pirate"
                    }
                },
                "type": "object",
                "required": [
                    "text",
                    "tone"
                ],
                "title": "ParaphraseRequest"
            },
            "ParaphraseResponse": {
                "properties": {
                    "paraphrase": {
                        "type": "string",
                        "title": "Paraphrase",
                        "description": "The generated paraphrased text",
                        "example": "Ahoy matey! That swift amber fox be leapin' over the sleepy hound whilst tossin' blazin' sticks in the air!"
                    }
                },
                "type": "object",
                "required": [
                    "paraphrase"
                ],
                "title": "ParaphraseResponse"
            },
            "src__routes__v1__pdf__schemas__SummarizeRequest": {
                "properties": {
                    "file_url": {
                        "type": "string",
                        "title": "File Url"
                    }
                },
                "type": "object",
                "required": [
                    "file_url"
                ],
                "title": "SummarizeRequest"
            },
            "src__routes__v1__text__schemas__ClassifyRequest": {
                "properties": {
                    "text": {
                        "type": "string",
                        "title": "Text",
                        "description": "The text content to be classified",
                        "example": "This product exceeded my expectations! The quality is excellent and it arrived faster than expected. Highly recommend!"
                    },
                    "categories": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array",
                        "title": "Categories",
                        "description": "List of categories to classify the text into",
                        "example": [
                            "positive",
                            "negative",
                            "neutral"
                        ]
                    }
                },
                "type": "object",
                "required": [
                    "text",
                    "categories"
                ],
                "title": "ClassifyRequest"
            },
            "src__routes__v1__text__schemas__ClassifyResponse": {
                "properties": {
                    "category": {
                        "type": "string",
                        "title": "Category",
                        "description": "The predicted category",
                        "example": "positive"
                    },
                    "confidence": {
                        "type": "number",
                        "title": "Confidence",
                        "description": "Confidence score for the prediction",
                        "example": 0.97
                    }
                },
                "type": "object",
                "required": [
                    "category",
                    "confidence"
                ],
                "title": "ClassifyResponse"
            },
            "src__routes__v1__text__schemas__SummarizeRequest": {
                "properties": {
                    "text": {
                        "type": "string",
                        "title": "Text",
                        "description": "The text content to be summarized",
                        "example": "Artificial intelligence has made remarkable progress in recent years, transforming industries from healthcare to finance. Machine learning algorithms can now analyze vast amounts of data to identify patterns and make predictions with unprecedented accuracy. However, as AI systems become more powerful, concerns about ethics, bias, and transparency have also grown. Researchers and policymakers are working together to develop frameworks that ensure AI is developed responsibly and benefits society as a whole."
                    }
                },
                "type": "object",
                "required": [
                    "text"
                ],
                "title": "SummarizeRequest"
            },
            "src__routes__v1__text__schemas__SummarizeResponse": {
                "properties": {
                    "summary": {
                        "type": "string",
                        "title": "Summary",
                        "description": "The generated summary",
                        "example": "AI has advanced significantly, impacting multiple industries through improved data analysis and predictions, while raising important concerns about ethics and responsible development."
                    }
                },
                "type": "object",
                "required": [
                    "summary"
                ],
                "title": "SummarizeResponse"
            },
            "src__routes__v1__url__schemas__ClassifyRequest": {
                "properties": {
                    "url": {
                        "type": "string",
                        "title": "URL",
                        "description": "The URL to classify content from",
                        "examples": [
                            "https://techcrunch.com/2024/01/15/ai-startup-news"
                        ]
                    },
                    "categories": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array",
                        "title": "Categories",
                        "description": "List of possible categories to classify into",
                        "examples": [
                            [
                                "Technology",
                                "Business",
                                "Science",
                                "Sports"
                            ]
                        ]
                    }
                },
                "type": "object",
                "required": [
                    "url",
                    "categories"
                ],
                "title": "ClassifyRequest"
            },
            "src__routes__v1__url__schemas__ClassifyResponse": {
                "properties": {
                    "category": {
                        "type": "string",
                        "title": "Category",
                        "description": "The predicted category for the content",
                        "examples": [
                            "Technology"
                        ]
                    },
                    "confidence": {
                        "type": "number",
                        "title": "Confidence",
                        "description": "Confidence score for the classification (0.0 to 1.0)",
                        "examples": [
                            0.92
                        ]
                    }
                },
                "type": "object",
                "required": [
                    "category",
                    "confidence"
                ],
                "title": "ClassifyResponse"
            },
            "src__routes__v1__url__schemas__SummarizeRequest": {
                "properties": {
                    "url": {
                        "type": "string",
                        "title": "URL",
                        "description": "The URL string for the summarize",
                        "examples": [
                            "https://en.wikipedia.org/wiki/Dinosaur"
                        ]
                    }
                },
                "type": "object",
                "required": [
                    "url"
                ],
                "title": "SummarizeRequest"
            },
            "src__routes__v1__url__schemas__SummarizeResponse": {
                "properties": {
                    "output": {
                        "type": "string",
                        "title": "Output"
                    }
                },
                "type": "object",
                "required": [
                    "output"
                ],
                "title": "SummarizeResponse"
            }
        }
    }
}