{
    "openapi": "3.1.0",
    "info": {
        "title": "Loor AI API",
        "version": "1.0.0"
    },
    "paths": {
        "/v1/text/summarize": {
            "post": {
                "tags": [
                    "Text"
                ],
                "summary": "Summarize Text",
                "description": "Summarize the provided text content.\n\nTakes text input and returns a concise summary using the configured\nsummarization model.\n\n- **text**: The text content to summarize",
                "operationId": "summarize_v1_text_summarize_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/src__routes__v1__text__schemas__SummarizeRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/src__routes__v1__text__schemas__SummarizeResponse"
                                }
                            }
                        }
                    },
                    "4XX": {
                        "description": "Error occurred while processing the request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/text/paraphrase": {
            "post": {
                "tags": [
                    "Text"
                ],
                "summary": "Paraphrase Text",
                "description": "Paraphrase the provided text content.\n\nTakes text input and returns a reworded version that maintains the\noriginal meaning while using different words and sentence structure.\n\n- **text**: The text content to paraphrase\n- **tone**: The desired tone for the paraphrased text",
                "operationId": "paraphrase_v1_text_paraphrase_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ParaphraseRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ParaphraseResponse"
                                }
                            }
                        }
                    },
                    "4XX": {
                        "description": "Error occurred while processing the request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/text/correct": {
            "post": {
                "tags": [
                    "Text"
                ],
                "summary": "Correct Text",
                "description": "Correct grammar and spelling errors in text.\n\nTakes text input and returns a corrected version with fixed grammar,\nspelling, and punctuation errors.\n\n- **text**: The text content to be corrected",
                "operationId": "correct_v1_text_correct_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CorrectRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CorrectResponse"
                                }
                            }
                        }
                    },
                    "4XX": {
                        "description": "Error occurred while processing the request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/text/classify": {
            "post": {
                "tags": [
                    "Text"
                ],
                "summary": "Classify Text",
                "description": "Classify text into predefined categories.\n\nAnalyzes the provided text and assigns it to the most appropriate\ncategory with a confidence score.\n\n- **text**: The text content to classify\n- **categories**: List of possible categories to classify into",
                "operationId": "classify_v1_text_classify_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/src__routes__v1__text__schemas__ClassifyRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/src__routes__v1__text__schemas__ClassifyResponse"
                                }
                            }
                        }
                    },
                    "4XX": {
                        "description": "Error occurred while processing the request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
